{
    "name": "homsa",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "devDependencies": {
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "scripts": {
        "dev": ".devserver/dev-up.sh",
        "dashboard": "bunx oh-my-opencode-dashboard@latest",
        "dashboard:add": "bunx oh-my-opencode-dashboard@latest add --name opencode-supernova",
        "toy:init": "bun run script/toy-bot-init.ts",
        "toy:cmd": "bun run script/toy-bot-command.ts",
        "toy:watch": "bun run script/toy-bot-watch.ts",
        "xoc:prompt": "bun run script/xoc-prompt.ts",
        "xoc:monitor": "bun run script/xoc-x4-monitor.ts",
        "xoc:session": "bun run script/xoc-session-measure.ts",
        "x2:worker": "bun run .devserver/x2/worker.ts",
        "x2:once": "bun run .devserver/x2/worker.ts --once",
        "x3:worker": "bun run .devserver/x3/worker.ts",
        "x3:once": "bun run .devserver/x3/worker.ts --once",
        "eq1:smoke": "cd .devserver && bun run eq1/smoke.ts"
    }
}
